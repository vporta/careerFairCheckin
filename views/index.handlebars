

<form id="sign-in-form" class="form-horizontal" action="/users/save" method="POST">
  <h2 style="margin: 100px">Welcome! Check In To Receive Resume.</h2>  

  <div class="form-group">
    <label for="FirstName" class="col-sm-2 control-label">First Name</label>
    <div class="col-sm-6">
    <input type="text" name="fname" placeholder="First name" class="form-control" > 
    </div>
  </div>

  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Email</label>
    <div class="col-sm-6">
      <input type="email" class="form-control" id="inputEmail" name="email" placeholder="Email">
    </div>
  </div>

  <div class="form-group">
    <label for="company" class="col-sm-2 control-label">Company</label>
    <div class="col-sm-6">
      <input type="text" class="form-control" id="inputPass" name="company" placeholder="Company Name">
    </div>
  </div>

  <div class="form-group">
    <label for="company" class="col-sm-2 control-label">Title</label>
    <div class="col-sm-6">
      <input type="text" class="form-control" id="inputPass" name="title" placeholder="Your Title">
    </div>
  </div>

  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">Submit</button><br>
    </div>
  </div>
</form>
<div class="row">
<div class="col-lg-12">
<table class="table" align="left">
  <thead>
    <tr class="active">
        <th align="left">Name</th>
        <th align="left">Email</th>
        <th align="left">Company</th>
        <th align="left">Title</th>
    </tr>
  </thead>
  <tbody>
{{#each data.users}}
    <tr>
        <td class="success" align="left">{{name}}</td>
        <td class="success" align="left">{{email}}</td>
        <td class="success" align="left">{{company}}</td>
        <td class="success" align="left">{{title}}</td>
      
    </tr>
{{/each}}
  </tbody>
</table>
</div>
</div>